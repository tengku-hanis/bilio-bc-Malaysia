@CONFERENCE{AbdulHameed2017,
author={Abdul Hameed, A.M. and Abdul Hamid, A.F. and Noor, N.S. and Appalanaido, G.K. and Sahul Hamid, S.B.},
title={Study on IL-2 and CA 15-3 level as combined biomarkers in monitoring chemotherapeutic response among invasive breast cancer patients},
journal={Journal of Physics: Conference Series},
year={2017},
volume={851},
number={1},
doi={10.1088/1742-6596/851/1/012038},
art_number={012038},
note={cited By 1; Conference of 10th International Seminar on Medical Physics, ISMP 2016 and 1st AMDI International Oncology Symposium, IOS 2016 ; Conference Date: 26 August 2016 Through 28 August 2016;  Conference Code:128333},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85021734464&doi=10.1088%2f1742-6596%2f851%2f1%2f012038&partnerID=40&md5=11b2f657200dd29a5cfd84865609f7d8},
affiliation={Advanced Medical and Dental Institute (AMDI), Universiti Sains Malaysia, Kepala Batas, Penang, 13200, Malaysia},
abstract={In Malaysia, breast cancer is the most frequent type of disease among women. This study was designed to determine the clinical usefulness of carbohydrate antigen (CA 15-3) and interleukin 2 (IL-2) levels as combined biomarkers in monitoring breast cancer patient's response to chemotherapy. Ethical approval was obtained to recruit patients with histologically confirmed invasive ductal carcinoma (IDC) attending Oncology Clinic at Advanced Medical and Dental Institute. Whole blood was collected from 10 IDC breast cancer patients' pre and post primary chemotherapy. Plasma was separated from the whole blood to determine the CA 15-3 level and IL-2 level using enzyme-linked immunosorbent assay (ELISA) pre and post-treatment. In addition, the histological findings, tumour stage and other patients' data were obtained from the medical record. Findings showed that IL-2 had borderline significant changes between pre- and post-chemotherapy (p = 0.074) whereas for CA 15-3, there was insignificant differences of CA 15-3 level between pre and post-chemotherapy (p > 0.05). It was noted that only CA 15-3 level had significant correlation with tumour size. This study demonstrates that IL-2 level requires further investigation in a larger sample size to correlate its potential use as combined biomarker with CA 15-3 in monitoring response to chemotherapy. © Published under licence by IOP Publishing Ltd.},
keywords={Blood;  Chemotherapy;  Diseases;  Oncology;  Tumors, Breast Cancer;  Enzyme linked immunosorbent assay;  Interleukin-2;  Invasive ductal carcinomata;  Medical record;  Post treatment;  Sample sizes;  Whole blood, Biomarkers},
