@ARTICLE{Liaw2020469,
author={Liaw, Y.Y. and Loong, F.S. and Tan, S. and On, S.Y. and Khaw, E. and Chiew, Y. and Nordin, R. and Mat, T.N. and Arulanantham, S. and Gandhi, A.},
title={A retrospective study on breast cancer presentation, risk factors, and protective factors in patients with a positive family history of breast cancer},
