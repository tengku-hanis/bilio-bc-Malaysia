document_type={Article},
source={Scopus},
}

@ARTICLE{Teo201386,
author={Teo, S.H. and Sook-Yee, Y. and Keong, T.M. and Yee, P.S. and Kang, P. and Nee, K.I. and Hassan, N. and Hasmad, H. and Sivanandan, K. and Mariapun, S. and Ling, W.Y. and Yip, C.H. and Taib, N.A.M.},
title={Genetic predisposition to breast cancer in Malaysian breast cancer patients: Update from the Malaysian Breast Cancer Genetic Study (MyBrCa)},
journal={Journal of the University of Malaya Medical Centre},
