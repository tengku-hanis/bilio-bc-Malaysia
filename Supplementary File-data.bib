@ARTICLE{Sa’at2021,
author={Sa’at, H. and Lee, Y.-K. and Yoon, S.-Y. and Wong, S.W. and Woo, Y.L. and Barlow-Stewart, K. and Mohd Taib, N.A.},
title={Decision-making for Risk-reducing Salpingo-oophorectomy (RRSO) in Southeast Asian BRCA Mutation Carriers With Breast Cancer: A Qualitative Study},
