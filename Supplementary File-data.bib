@ARTICLE{Yaw20101535,
author={Yaw, Y.H. and Kandiah, M. and Shariff, Z.M. and Mun, C.Y. and Hashim, Z. and Yusof, R.M. and Othman, Z. and Saibul, N. and Weay, Y.H.},
title={Pattern of weight changes in women with breast cancer},
journal={Asian Pacific Journal of Cancer Prevention},
year={2010},
